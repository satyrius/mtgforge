<div class="">
    <h1>Magic: The Gathering cards intelligent search tool</h1>
    <p class="lead">Just start typing your search query in the box and see magic.</p>
    <h2>Choose a product to explore</h2>
    <div id="products-list">
    <% card_sets.each(function(cs) { %>
        <a class="card-set"
            data-acronym="<%= cs.get("acronym") %>"
            href="#<%= cs.getSearchUrl() %>"
        >
            <%= cs.get("name") %>
        </a>
    <% }) %>
    </div>
</div>
